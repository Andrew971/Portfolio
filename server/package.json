{
  "name": "portfolio-server",
  "version": "0.0.0",
  "private": true,
  "proxy": "http://localhost:8080/api/",
  "homepage": "http://localhost:8080/api/",
  "scripts": {
    "start": "pm2 start ./bin/www && pm2 startup && pm2 save",
    "server": "nodemon ./bin/www",
    "staging": "yarn build && NODE_ENV=staging nodemon ./bin/www",
    "prod": "yarn build && NODE_ENV=production nodemon ./bin/www",
    "s3-deploy": "aws s3 sync ./ s3://andrewsordier-server-bucket --exclude 'node_modules/*",
    "s3-download": "sudo aws s3 sync s3://andrewsordier-server-bucket /var/server/ && npm install",
    "deploy": "yarn pre-d && eb deploy",
    "analyze": "cd ./client && yarn analyze",
    "client-i": "cd client && yarn install",
    "i-all": "yarn client-i && yarn install"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-mailer": "^0.3.1",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "npm-run-all": "^4.1.2"
  },
  "description": "this is my boilerplate user express + create-react-app + redux",
  "main": "index.js",
  "license": "MIT"
}
